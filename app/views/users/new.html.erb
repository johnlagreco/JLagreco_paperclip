<%= form_for @user, :url => users_path, :html => { :multipart => true } do |form| %>
  <%= form.label :username %>
  <%= form.text_field :username %><br><br>  
  <%= form.file_field :avatar %> <br><br>
  <%= form.submit %>
<% end %>
<br><br><br><br><br><br>
<%= image_tag @user.avatar.url(:medium) %>