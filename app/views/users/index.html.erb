<h1>Welcome Uploaders!</h1>

<%= link_to 'New User', new_user_path %>
<br><br><br><br>
	 <% @user.each do |user| %>
        <%= image_tag user.avatar.url(:thumb)%><br>
        <%= user.username %><br>
       	<%= link_to 'My Page', user %>
        <%= link_to 'Delete', user, method: :delete, data: { confirm: 'Are you sure?' } %><br><br>
    <% end %>
